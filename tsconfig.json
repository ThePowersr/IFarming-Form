{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-native",
    "lib": [
      "ES6",
      "DOM"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "target": "ES2017"
  },
  "exclude": [
    "node_modules"
  ]
}